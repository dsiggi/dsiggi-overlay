/sys/class/graphics/fb0/request2XScale \
/sys/class/graphics/fb0/scale_width \
/sys/class/graphics/fb0/scale_height \
/sys/class/graphics/fb1/blank \
/sys/class/graphics/fb1/scale_width \
/sys/class/graphics/fb1/scale_height \
/sys/class/graphics/fb1/free_scale \
